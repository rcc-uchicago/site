	if (typeof ss_icons !== 'object' || typeof ss_icons !== 'object') {
		var ss_icons = ss_set;
		var ss_keywords = [];
		for (var i in ss_set) { ss_keywords.push(i); };
	} else {
		for (var i in ss_set) { ss_icons[i] = ss_set[i]; ss_keywords.push(i); }
	};

