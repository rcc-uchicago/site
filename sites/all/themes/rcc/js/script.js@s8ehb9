var $rcc = jQuery.noConflict(true)

// Finally, include our jQuery
$rcc( document ).ready(function() {

  // Initialize popups.
  $rcc('a[rel^=lightbox], a.popup').magnificPopup({
  	type: 'image',
  	closeOnContentClick: true,
  	mainClass: 'mfp-img-mobile mfp-with-zoom',
  	image: {
  		verticalFit: true
  	}
  });

  // Mobile nav - toggle, bootstrap-esque
  $rcc('.nav-button').click(function(e) {
  	console.log('clicked');
  	$rcc('.region-sidebar-first').hide();
  	$rcc('.top-nav-inner').slideToggle();
  	$rcc(this).toggleClass('on');
  });

  // Define a function which will ensure that all landing page feature blocks have the same height, matching the height of the tallest block.
  function resizeFeatureBlocks() {
  	var maxHeight = 0;

  	$rcc('.node-type-landing-page .feature-block').each(function() {
  		$rcc(this).removeAttr('style');
  		if ($rcc(this).height() > maxHeight) { maxHeight = $rcc(this).height(); }
  	});

  	$rcc('.node-type-landing-page .feature-block').height(maxHeight);
  }

  // Resize all the blocks on window load.
  $rcc(window).load(function() {
  	resizeFeatureBlocks();
  });

  // And then resize them whenever the browser is resize, just to make sure nothing looks horribly broken.
  $rcc(window).on('resize', function() {
  	resizeFeatureBlocks();
  });


  // ** MOBILE **
  // give body .small-screen to only run mobile scripts on smaller screens
  function pageWidth() {
  	return window.innerWidth != null? window.innerWidth: document.body != null? document.body.clientWidth:null;
  }

  if (pageWidth() < 768) {

    // Build the section navigation
    var active_page = $rcc('.region-sidebar-first .menu li.active');
    if (active_page.length) {
    	var section_menu = active_page.find('ul').html();
    	if (section_menu) {
    		section_menu = '<ul class="menu">' + section_menu + '</ul>';
    		$rcc('.region-sidebar-first .menu-block-wrapper').html(section_menu);
    	} else {
    		$rcc('.mobile-section-navigation').hide();
    	}
    }

    $rcc('.mobile-section-navigation').click(function() {
    	$rcc('.region-sidebar-first').slideToggle(); 
    	var arrow = $rcc(this).find('span').html();
    	if (arrow == 'navigateright') {
    		$rcc(this).find('span').html('navigatedown');
    	} else {
    		$rcc(this).find('span').html('navigateright');
    	}
    })
}

});

// numbers widget related stuff

$rcc('document').ready(function(){
	$rcc("#numbers-widget ul li:last-child").remove();
	var $numbersWidget = $rcc('#numbers-widget ul');	

	$numbersWidget.on('cycle-post-initialize', function(event, optionHash) {

		$rcc.getJSON("/sites/all/themes/rcc/js/numbers.json?" + new Date().getTime(), function(json){ 
			$thisSlide = $rcc(document).find('li[data-slide-number="0"]');
			$thisSlide.find('h3').text(json[0].value);
			$thisSlide.find('i').text(json[0].icon);
			$thisSlide.find('a').attr('href', json[0].path);


			var thisSlideCurrValue = $thisSlide.attr('data-slide-current-value');

			if (thisSlideCurrValue == "0") {
				thisSlideCurrValue = json[0].startNumber;
			}


			var thisSlideDelta = json[0].delta;	
			var thisSlideEndValue = thisSlideCurrValue + parseInt(thisSlideDelta);	
			var comma_separator_number_step = $rcc.animateNumber.numberStepFactories.separator(',');

			$thisSlide.find('p').prop('number', thisSlideCurrValue)
			.animateNumber(
			{
				number: thisSlideEndValue,
				numberStep: comma_separator_number_step
			},
			4000
			);
		});
		
		$numbersWidget.cycle('pause');
	});

	$numbersWidget.cycle({
		slideActiveClass: 'active-slide',
		pauseOnHover: true,
		timeout: 5000,
		autoHeight: false
    }); // initialize Cycle, then pause until it's scrolled into view	

	$rcc('#numbers-widget').waypoint({
		offset: 'bottom-in-view',
		triggerOnce: true,
		handler: function(direction){
			$numbersWidget.cycle('resume');         
		}
	});

	$numbersWidget.on('cycle-after', function(event, optionHash) { // after each slide is done transitioning, animate the #s
		$thisSlide = $rcc(this).find('li.active-slide');

		$rcc('body').find('.thisSlide').removeClass('thisSlide');
		$thisSlide.addClass('thisSlide');

		thisSlideNumber = optionHash.currSlide;

		//$nextSlide = $thisSlide.next();
		$nextSlide = $rcc(document).find('li[data-slide-number='+ optionHash.nextSlide +']')
		nextSlideNumber = optionHash.nextSlide;
		if (thisSlideNumber == "2") { // if this is the last slide, reset
			nextSlideNumber = "0";
		}
		$rcc('body').find('.nextSlide').removeClass('nextSlide');
		$nextSlide.addClass('nextSlide');

// populate the slides with values from numbers.json
$rcc.getJSON("/sites/all/themes/rcc/js/numbers.json?" + new Date().getTime(), function(json){ 

	$nextSlide.find('h3').text(json[optionHash.nextSlide].value);
	$nextSlide.find('i').text(json[optionHash.nextSlide].icon);
	$nextSlide.find('a').attr('href', json[optionHash.nextSlide].path);

	var nextSlideCurrValue = $nextSlide.attr('data-slide-current-value');
	nextSlideCurrValue = parseInt(nextSlideCurrValue.replace(/,/g, ''), 10);
	
	if (nextSlideCurrValue == "0") {
		nextSlideCurrValue = json[optionHash.nextSlide].startNumber;
	}
	var nextSlideDelta = json[optionHash.nextSlide].delta;	
	var nextSlideEndValue = nextSlideCurrValue + parseInt(nextSlideDelta);	
	var comma_separator_number_step = $rcc.animateNumber.numberStepFactories.separator(',');

	$nextSlide.find('p').prop('number', nextSlideCurrValue)
	.animateNumber(
	{
		number: nextSlideEndValue,
		numberStep: comma_separator_number_step
	},
	4000
	);

});

$numbersWidget.on('cycle-before', function(event, optionHash) {
	thisSlideCurrValue = parseInt($thisSlide.find('p').text().replace(/,/g, ''));

	$thisSlide.attr('data-slide-current-value', thisSlideCurrValue);
	thisSlideCurrValue = 0;

});
});

});

// interior page background color for title

var titleBgColor = $rcc(document).find('article').attr('data-background-color');
if(!titleBgColor) {titleBgColor = "red";};
$rcc(document).find('.page-title-wrapper, .page-title-wrapper .page__title, .triangle-bottom-right').addClass(titleBgColor);

// interior page shape fix for certain widths/title lengths where the h2 wraps to two lines.

$rcc(window).load(fixTitle);
//$rcc(window).resize(fixTitle);
$rcc(window).smartresize(fixTitle);

function fixTitle() {

	var pageTitleHeight = $rcc('.page-title-wrapper').height();	
	if (pageTitleHeight <= 75) {
		$rcc('body').find('.page-title-wrapper .triangle-bottom-right').show();
		var pageTitleHeight = $rcc('.page-title-wrapper').height();	
	} else {
		$rcc('body').find('#page-title').attr('style','font-size: 22px;');		
		var borderTopWidth = $rcc('#page-title').outerHeight(true);		
		$rcc('body').find('.page-title-wrapper .triangle-bottom-right').attr('style','border-top-width: ' + borderTopWidth + 'px;');
		var pageTitleHeight = $rcc('.page-title-wrapper').height();			
	}
	if (pageTitleHeight > 75) {
		$rcc('body').find('.page-title-wrapper .triangle-bottom-right').hide();
		var pageTitleHeight = $rcc('.page-title-wrapper').height();	
	}	
}

// slideshow arrow controls

$rcc(window).load(function(){
	$rcc('.slideshow-slides-container').find('.cycle-control').delay(1000).fadeOut('slow');
});

$rcc('.feature-slides-container, .slideshow-slides-container').hoverIntent({
	over: showArrows,
	out: hideArrows,
	sensitivity: 10,
	timeout: 150
}, function(){});

function showArrows() {
	$rcc(this).find('.cycle-control.arrow').fadeIn('fast');
}

function hideArrows() {
	$rcc(this).find('.cycle-control.arrow').fadeOut('fast');
}

$rcc(document).ready(function(){
	var firstColor = $rcc('.front .cycle-slideshow').find('h1').attr('class');
	$rcc(this).find('.feature-pager span:first-child').addClass(firstColor);
});

$rcc('.front .cycle-slideshow').on('cycle-before', function(event, optionHash) {
	// console.log(optionHash.nextSlide);
	var activeColor = $rcc(this).find('.cycle-slide[data-slide-number="' + optionHash.nextSlide + '"]').find('h1').attr('class');
	//console.log(activeColor);
	$rcc(this).parents('.feature-slides-container').find('.cycle-pager-active').next().addClass(activeColor);
});

$rcc('.cycle-control.next').on('click', function(event){
	event.preventDefault();
	$rcc('.cycle-slideshow').cycle('next');
});
$rcc('.cycle-control.prev').on('click', function(event){
	event.preventDefault();
	$rcc('.cycle-slideshow').cycle('prev');
});

// Show and hide nav dropdowns

function toggleNavIn() {
	if ( $rcc(window).width() > 767) {
		toggleNavOut();
    //$rcc(this).parents('.header').find('.rich-nav').toggleClass('hover').find('ul').fadeIn(200);
    $rcc(this).parents('not-front').find('li').find('.triangle').fadeIn('fast');
    var thisMenuBlockID = $rcc(this).siblings('.menu-block-id').attr('data-menu-block-id');
    $rcc(document).find('.rich-nav-container[data-menu-block-id="'+ thisMenuBlockID +'"]').toggleClass('hover').fadeIn('fast');
}
}

function toggleNavOut() {
	$rcc(document).find('.rich-nav-container.hover').toggleClass('hover').hide();
	$rcc(document).find('.menu .triangle').hide();
}

$rcc('.top-nav-inner a').hoverIntent({
	over: toggleNavIn, 
	out: function(){}, 
	sensitivity: 10,
	timeout: 150
}, function(){});

$rcc('.rich-nav-container').hoverIntent({
	over: function(){},
	out: toggleNavOut,
	sensitivity: 300
}, function(){});

// search focus style

$rcc('input#searchterm').focus(function(){
	$rcc('#tinytriangle-border, .search-wrapper').addClass('search-focus');
});

$rcc('input#searchterm').focusout(function(){
	$rcc(document).find('.search-focus').removeClass('search-focus');
});

// fix sidebar padding and bgs

$rcc(window).load(function(){
	$rcc('.content-inner-sidebar p').each(function(){
		var str = $rcc(this).html();
		if (str.indexOf("img") >= 0) {
			$rcc(this).attr('style','padding: 0;');
			$rcc(this).parents('.content-inner-sidebar').attr('style','background: white;');
		}

	});
});

// sidebar thumbnail overlays

$rcc(window).load(function(){
	$rcc('.content-inner-sidebar p').each(function(){
		$rcc(this).find('a img').each(function(){

			thisHref = $rcc(this).parents('a').attr('href');
			if (thisHref.indexOf("https://www.youtube.com") >= 0) {
				var playerIcon = "<span class='player-icon ss-icon'>play</span>";				
				$rcc(this).parents('a').attr('style','display: block; position: relative;').append(playerIcon);

			}
		});

	});
});

// top nav active link

$rcc('.desktop .not-front .view-display-id-block_1 a').each(function(){
	var thisSection = $rcc(this).attr('id');
	if ($rcc('body').hasClass(thisSection)) {
		$rcc(this).addClass('active-trail');
	}
});
