	};

	var ss_liga = function(that) {
		var re = new RegExp(ss_keywords.join('|').replace(/[-[\]{}()*+?.,\\^$#\s]/g, "\\$&"),"gi");
		return that.replace(re, function(v) {
			return ss_icons[v.toLowerCase()];
		});
	};

